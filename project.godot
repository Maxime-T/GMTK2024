[application]

config/name="GMTK2024"
run/main_scene="res://Menu/menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://HUD items/Logo_Tat_blanc.png"
boot_splash/fullsize=false
config/icon="res://Menu/Logo.png"
boot_splash/minimum_display_time=1500

[autoload]

Global="*res://Auto loaded/Global.gd"
Music="*res://Auto loaded/music.tscn"
GlobalSignals="*res://Auto loaded/GlobalSignals.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

